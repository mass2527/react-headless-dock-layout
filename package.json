{
  "name": "react-headless-dock-layout",
  "version": "0.2.1",
  "description": "A lightweight, headless dock layout library for React.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "dev": "vite",
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "headless",
    "dock layout"
  ],
  "files": [
    "dist"
  ],
  "license": "ISC",
  "packageManager": "pnpm@9.5.0+sha1.8c155dc114e1689d18937974f6571e0ceee66f1d",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
